
public class HelloWorld {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

		int i = 10;
		String a = "田中";

		switch (i) {
		case 1:
			System.out.println("1です");
			break;

		case 2:
			System.out.println("2です");
			break;

		default:
			System.out.println("該当無しです");
		}

		switch (a) {
		case "田中":
			System.out.println("田中さんです");
			break;

		case "山本":
			System.out.println("山本さんです");
			break;

		default:
			System.out.println("該当無しです");
		}
	}

}
